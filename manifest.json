{
    "manifest_version": 2,
    "name": "Note Taker",
    "version": "1.0",
    "description": "This Extension is design to take the Notes . ",
    "icons": {
        "128": "icon128.jpg",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "storage"
    ]
}